{
  "key": "firebaseCloudMessagingConnector",
  "name": "Firebase Cloud Messaging",
  "version": "1.0.0",
  "platformVersion": "1.0.0",
  "actions": [
    {
      
      "key": "fcmPushNotification",
      "name": "Send push notification",
      "display": {
        "label": "Send push notification",
        "description": ""
      },
      "operation": {
        "type": "api",
        "operation": {
          "url": "ws://grindery-ping-connector:3000/ws/"
        },
        "inputFields": [
          {
            "key": "title",
            "label": "Notification title",
            "type": "string",
            "required": false,
            "placeholder": "Enter optional title"
          },
          {
            "key": "body",
            "label": "Notification text",
            "type": "string",
            "required": true,
            "placeholder": "Enter notification text"
          },
          {
            "key": "icon",
            "label": "Icon",
            "type": "url",
            "required": false,
            "placeholder": "",
            "helpText": "Image URL. Supports JPEG or PNG format. Recommended image size at least 50x50px."
          },
          {
            "key": "tokens",
            "label": "FCM tokens",
            "type": "string",
            "list": true,
            "required": true,
            "placeholder": ""
          },
          {
            "key": "data",
            "label": "Data",
            "type": "string",
            "dict": true,
            "required": false,
            "placeholder": "",
            "helpText": "Custom data as a serialized JSON string. Will be passed as a 'data' property in a notification payload."
          }
        ],
        "outputFields": [
        ]
      }
    },
